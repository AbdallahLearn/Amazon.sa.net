@model List<AmazonSA.Models.Order>
@{
    ViewData["Title"] = "History";
}
<h2 class="text-center my-4">Your Order History</h2>
<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderID</td>
                <td>@order.OrderDate.ToShortDateString()</td>
                <td>@order.Status</td>
            </tr>
        }
    </tbody>
</table>
